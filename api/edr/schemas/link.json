{"link": {
    "type": "object",
    "required": [
      "href",
      "rel"
    ],
    "properties": {
      "href": {
        "type": "string"
      },
      "rel": {
        "type": "string",
        "example": "prev"
      },
      "type": {
        "type": "string",
        "example": "application/geo+json"
      },
      "hreflang": {
        "type": "string",
        "example": "en"
      },
      "title": {
        "type": "string",
        "example": "Monitoring site name"
      },
      "length": {
        "type": "integer"      
      }
    }
}}