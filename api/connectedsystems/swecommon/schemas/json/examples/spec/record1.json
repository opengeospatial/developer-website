{
  "type": "DataRecord",
  "label": "Weather Data Record",
  "fields": [
    {
      "name": "time",
      "type": "Time",
      "definition": "http://www.opengis.net/def/property/OGC/0/SamplingTime",
      "referenceFrame": "http://www.opengis.net/def/trs/BIPM/0/UTC",
      "label": "Sampling Time",
      "uom": {
        "href": "http://www.opengis.net/def/uom/ISO-8601/0/Gregorian"
      }
    },
    {
      "name": "temperature",
      "type": "Quantity",
      "definition": "http://mmisw.org/ont/cf/parameter/air_temperature",
      "label": "Air Temperature",
      "uom": { "code": "Cel" }
    },
    {
      "name": "pressure",
      "type": "Quantity",
      "definition": "http://mmisw.org/ont/cf/parameter/air_pressure_at_mean_sea_level",
      "label": "Air Pressure",
      "uom": { "code": "mbar" }
    },
    {
      "name": "windSpeed",
      "type": "Quantity",
      "definition": "http://mmisw.org/ont/cf/parameter/wind_speed",
      "label": "Wind Speed",
      "uom": { "code": "km/h" }
    },
    {
      "name": "windDirection",
      "type": "Quantity",
      "definition": "http://mmisw.org/ont/cf/parameter/wind_to_direction",
      "label": "Wind Direction",
      "uom": { "code": "deg" }
    }
  ]
}